    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.5.0",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.3.1",

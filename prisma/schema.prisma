generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Greeting {
  id       Int    @id() @default(autoincrement())
  greeting String
  name     String
}

model Faq {
  id       Int     @id() @default(autoincrement())
  question String
  answer   String
  category String?
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Greeting {
  id       Int    @id() @default(autoincrement())
  greeting String
  name     String
}

model Faq {
  id       Int     @id() @default(autoincrement())
  question String
  answer   String
  category String?
}

model Club {
  id          Int       @id() @default(autoincrement())
  name        String
  description String
  projects    Project[]
}

model Project {
  id          Int    @id() @default(autoincrement())
  name        String
  description String
  club        Club   @relation(fields: [clubId], references: [id], onDelete: Cascade)
  clubId      Int
  image       String
}
